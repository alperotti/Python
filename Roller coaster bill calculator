print("Welcome to the rollercoaster!")
height = int(input("What is your height in cm? "))
age = int(input("What is your age? "))

bill = 0

if height > 120 and age < 55:

  photo = input("Do you want photo? 'Y' or 'N'")
  if age < 12:
    bill += 5
    
  elif age < 18:
    bill += 7

  elif age < 45:
    bill += 12

  if photo == "Y":
    bill += 3

  print(f"Your total bill is ${bill}")

else:
    print("You dont ride.")
